    "jquery": "^3.6.4",
    "jsonwebtoken": "^9.0.0",
    "jszip": "^3.10.1",
    "pdfmake": "^0.2.7",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.13",
    "vue-bootstrap-table": "^0.1.2",
    "vue-bootstrap-table2": "^1.1.8",
    "vue-router": "^4.0.3",
    "vuetable-2": "^2.0.0-beta.4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
